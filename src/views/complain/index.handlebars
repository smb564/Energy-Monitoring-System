<h1>Complaint</h1>
<div class="row">
    <ul class="collapsible">
        {{#each complains}}
        <li>
            <div class="collapsible-header blue lighten-5">{{this.title}}{{#if this.first_name}} - {{this.first_name}} {{this.last_name}}{{/if}}</div>
            <div class="collapsible-body">
                <p>{{this.description}}
                {{#if this.comment}}
                    <hr>
                    Admin Comment :- {{{this.comment}}}
                {{else}}
                    {{#if this.first_name}}
                        <p style="margin-left:50px"><a href="/complain/admin/{{this.id}}">Add Comment >></a></p>
                    {{else}}
                        <p style="margin-left:50px"><a href="/complain/{{this.id}}">View>></a></p>
                    {{/if}}
                {{/if}}
                </p>
            </div>
        </li>
        {{/each}}
    </ul>
</div>